{
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "cSpell.words": [
    "apng",
    "cachebuster",
    "Eldes",
    "eldesalarms",
    "gatecontrol",
    "gatesconfig",
    "KHTML",
    "LOGAPI",
    "lxml",
    "mktime",
    "nosync",
    "PHPSESSID",
    "Referer",
    "Usersdatabase",
    "validuntildate"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true,
  "python.testing.unittestArgs": ["-v", "-s", "./test", "-p", "*.py"],
  "python.testing.autoTestDiscoverOnSaveEnabled": true,
  "python.analysis.extraPaths": ["./src", "./test"]
}
